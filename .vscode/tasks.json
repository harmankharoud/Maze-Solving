{
  "version": "2.0.0",
  "command": "${workspaceRoot}/.vscode/build.bat",
  "args": [
    "-g",
    "solve.c"
  ],
  "tasks": [
    {
      "type": "shell",
      "label": "gcc build active file",
      "command": "/usr/bin/gcc",
      "args": [
        "${workspaceRoot}/project/mains/solve.c",
        "-g",
        "-o",
        "${workspaceRoot}/project/compiled/solve"
      ],
      "options": {
        "cwd": "/usr/bin"
      },
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}