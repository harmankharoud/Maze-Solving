{
    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "C++ Launch (Windows)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/project/compiled/solve.exe",
            "args": ["", "${workspaceRoot}/project/images/normal.png", "${workspaceRoot}/project/solvedimages/solvedImage.png"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": true
        },
        {
            "name": "C++ Attach (Windows)",
            "type": "cppvsdbg",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}